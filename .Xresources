URxvt.scrollBar: false
URxvt.font: xft:DejaVu Sans Mono:pixelsize=13:foundry=PfEd:style=Book
! Extentions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
! url-select
URxvt.keysym.M-u:	    perl:url-select:select_next
URxvt.url-select.launcher:  firefox-nightly
URxvt.url-select.underline: true
! matcher
URxvt.url-launcher: firefox-nightly
URxvt.matcher.button: 1
! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
! resize-font
URxvt.resize-font.smaller:	C-minus
URxvt.resize-font.bigger:	C-plus

! special
*.foreground:   #ffffff
*.background:   #000000
*.cursorColor:  #ffffff

! black
*.color0:       #000000
*.color8:       #363636

! red
*.color1:       #d40000
*.color9:       #ff2222

! green
*.color2:       #007900
*.color10:      #00aa00

! yellow
*.color3:       #cf4216
*.color11:      #ff4500

! blue
*.color4:       #005f87
*.color12:      #00aad4

! magenta
*.color5:       #8b09b1
*.color13:      #aa00d4

! cyan
*.color6:       #20786d
*.color14:      #2ea596

! white
*.color7:       #30536f
*.color15:      #ffffff



